<view class="history-container">
    <block wx:for-items="{{historyList}}" wx:for-item="page" wx:key="*item">
      
      <view data-id="{{page.id}}" bindtap="forward" class="navigator" role="navigator">
        <view class="navigator-text">{{page.text}}</view>
      <view class="navigator-arrow"></view>
      </view>
    </block>
       <view class="history-footer">
        <view class="history-buttons" bindtap="back">
          <text>返回</text>
        </view>
    </view>
</view>